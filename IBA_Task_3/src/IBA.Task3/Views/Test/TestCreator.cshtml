@{
    ViewData["Title"] = "CreateTest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Styles/Styles4.css" rel="stylesheet">

<div id="article">
    <form name="createForm" id="createForm">
        <h2>Создание нового теста</h2>
        <div>
            <label>Название теста</label>
            <input id="testName" type="text" required /><br /><br />
        </div>
        <div>
            <label>Выберите количество попыток</label>
            <input id="attempts" type="number" min="1" max="3" required /><br /><br />
        </div>
        <div id="card-body">
            <input type="submit" id="submit" value="Далее" /><br /><br />
        </div>
    </form>
    <div id="card-body">
        <input type="submit" id="submitFinish" value="Завершить" /><br /><br />
    </div>
</div>

<script src="~/Scripts/TestCreator.js"></script>





